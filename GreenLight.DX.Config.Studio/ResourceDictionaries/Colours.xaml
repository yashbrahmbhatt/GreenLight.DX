<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="PrimaryColor">#007bff</Color>
    <Color x:Key="SecondaryColor">#6c757d</Color>
    <Color x:Key="SuccessColor">#28a745</Color>
    <Color x:Key="DangerColor">#dc3545</Color>
    <Color x:Key="WarningColor">#ffc107</Color>
    <Color x:Key="InfoColor">#17a2b8</Color>
    <Color x:Key="LightColor">#f8f9fa</Color>
    <Color x:Key="DarkColor">#343a40</Color>
    <Color x:Key="HoverColor">#DDDDDD</Color>
    <Color x:Key="HoverBorderColor">#AAAAAA</Color>
    <Color x:Key="PressedColor">#999999</Color>
    <Color x:Key="PressedBorderColor">#777777</Color>

    <SolidColorBrush x:Key="PrimaryBrushColor" Color="{StaticResource PrimaryColor}" />
    <SolidColorBrush x:Key="SecondaryBrushColor" Color="{StaticResource SecondaryColor}" />
    <SolidColorBrush x:Key="SuccessBrushColor" Color="{StaticResource SuccessColor}" />
    <SolidColorBrush x:Key="DangerBrushColor" Color="{StaticResource DangerColor}" />
    <SolidColorBrush x:Key="WarningBrushColor" Color="{StaticResource WarningColor}" />
    <SolidColorBrush x:Key="InfoBrushColor" Color="{StaticResource InfoColor}" />
    <SolidColorBrush x:Key="LightBrushColor" Color="{StaticResource LightColor}" />
    <SolidColorBrush x:Key="DarkBrushColor" Color="{StaticResource DarkColor}" />
    <SolidColorBrush x:Key="HoverBrushColor" Color="{StaticResource HoverColor}" />
    <SolidColorBrush x:Key="HoverBorderBrushColor" Color="{StaticResource HoverBorderColor}" />
    <SolidColorBrush x:Key="PressedBrushColor" Color="{StaticResource PressedColor}" />
    <SolidColorBrush x:Key="PressedBorderBrushColor" Color="{StaticResource PressedBorderColor}" />

    
    

</ResourceDictionary>